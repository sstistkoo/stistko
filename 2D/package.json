{
  "name": "soustruznik-2d",
  "version": "1.0.0",
  "description": "Parametrické kreslení CAD aplikace s AI asistencí",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "dev": "echo 'Use VS Code Live Server extension'",
    "build": "echo 'Minify: npm install -g minifier && minify *.js *.css'",
    "test": "node tests/run-tests.cjs",
    "test:watch": "node -w tests/run-tests.cjs",
    "serve": "npx http-server",
    "format": "echo 'Add Prettier: npm install --save-dev prettier'"
  },
  "keywords": [
    "CAD",
    "Drawing",
    "Canvas",
    "AI",
    "Gemini",
    "Parametric",
    "Geometry",
    "Web Application"
  ],
